@{
    ViewBag.Title = "EditProgramYear";
}

<h2>Please Select a year to Edit</h2>
@using (Html.BeginForm("EditProgramYear", "Principal", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <div>
        <div class="form-group">
            <row>
              <div class="col-md col-md-10">
                @Html.DropDownList("Year", null, new { style = "width: 100%" })
              </div>
            </row>
            <row>
                <div class="col-md col-md-10">
                    <input type="submit" class="btn btn-default" value="Submit" />
                </div>
            </row>
        </div>

    </div>
}
